select distinct 
       ce.num_contrato,
			 /*'update ts.contrato_empresa ce set ce.data_inicio_vigencia = to_date(''01/04/2023'', ''dd/mm/yyyy'') where ce.cod_ts_contrato = ' || ce.cod_ts_contrato || ';', 
				
				'update ts.contrato_cobranca cc set cc.dt_ini_cobranca = to_date(''01/04/2023'', ''dd/mm/yyyy'') where cc.cod_ts_contrato = ' || ce.cod_ts_contrato || ';',
			 
			 'update ts.contrato_adesao_plano cp set cp.dt_ini_vigencia = to_date(''01/04/2023'', ''dd/mm/yyyy''), ' ||
			 'cp.dt_ini_validade = to_date(''01/04/2023'', ''dd/mm/yyyy'')where cp.cod_ts_contrato = ' || ce.cod_ts_contrato || ';',
			 
			 'update ts.contrato_plano cp set cp.dt_ini_vigencia = to_date(''01/04/2023'', ''dd/mm/yyyy'') where cp.cod_ts_contrato = ' || ce.cod_ts_contrato || ';',
			 
			 'update ts.VALOR_FE_CONTRATO cp set cp.dt_ini_vigencia = to_date(''01/04/2023'', ''dd/mm/yyyy'') where cp.cod_ts_contrato = ' || ce.cod_ts_contrato || ';',
			 
			 'update ts.contrato_plano_dependencia cp set cp.dt_ini_vigencia = to_date(''01/04/2023'', ''dd/mm/yyyy''), ' ||
			 'cp.dt_ini_validade = to_date(''01/04/2023'', ''dd/mm/yyyy'') where cp.cod_ts_contrato = ' || ce.cod_ts_contrato || ';'*/
			  
			 'update ts.beneficiario b set b.data_inclusao = to_date(''' || ce.data_inicio_vigencia || 
			 ''', ''dd/mm/yyyy'') where b.cod_ts = ' || b.cod_ts || ';'
from ts.contrato_empresa ce
		 join ts.beneficiario b on b.cod_ts_contrato = ce.cod_ts_contrato
where 1 = 1
       /*and exists (select 1
              from ts.contrato_cobranca cc
							where cc.cod_ts_contrato = ce.cod_ts_contrato
							      and cc.dt_ini_cobranca >= to_date('01/03/2023', 'dd/mm/yyyy'))*/
      and ce.num_contrato IN ('47065',
'47519',
